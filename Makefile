CC=gcc
CFLAGS=-Wall
MAINPROG=-o Sat
MAINFILE=sat.c


moser:
	$(CC) $(CFLAGS) $(MAINFILE) moser.c $(MAINPROG)

graphe1:
	$(CC) $(CFLAGS) $(MAINFILE) graphe1.c $(MAINPROG)

graphe2:
	$(CC) $(CFLAGS) $(MAINFILE) graphe2.c $(MAINPROG)

TD11:
	$(CC) $(CFLAGS) $(MAINFILE) TD1.1.c $(MAINPROG)

TD12:
	$(CC) $(CFLAGS) $(MAINFILE) TD1.2.c $(MAINPROG)

TD21:
	$(CC) $(CFLAGS) $(MAINFILE) TD2.1.c $(MAINPROG)

TD22:
	$(CC) $(CFLAGS) $(MAINFILE) TD2.2.c $(MAINPROG)


.PHONY: clean

clean:
	rm -f *.o *~
